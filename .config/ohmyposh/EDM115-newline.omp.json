{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "template": "  ❯❯ {{ .AbsolutePWD }} : ",
    "foreground": "p:color15",
    "background": "transparent",
    "newline": true
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\n╭─",
          "foreground": "p:color15",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "",
          "template": "{{ .UserName }}",
          "foreground": "p:color15",
          "background": "p:color13",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "󱐋",
          "foreground": "#ff5555",
          "powerline_symbol": "",
          "background": "p:color13",
          "type": "root",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ .Icon }}  ",
          "foreground": "p:color15",
          "powerline_symbol": "",
          "background": "p:color13",
          "type": "os",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "trailing_diamond": "",
          "template": " 󰉖 {{ path .Path .Location }}",
          "foreground": "p:color13",
          "background": "p:color10",
          "type": "path",
          "style": "powerline"
        },
        {
          "type": "project",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "p:color13",
          "background": "p:color10",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }} {{.Version}}{{ end }} {{ if .Name }}{{ .Name }}{{ end }}{{ end }} "
        },
        {
          "type": "upgrade",
          "style": "plain",
          "foreground": "p:color13",
          "background": "p:color10",
          "properties": {
            "cache_duration": "168h"
          }
        },
        {
          "type": "status",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "p:color13",
          "background": "p:color10",
          "background_templates": [
            "{{ if .Error }}#e91e63{{ end }}"
          ],
          "trailing_diamond": "",
          "template": "<#193549></>  ",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "battery",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "p:color13",
          "background": "p:color10",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#40c4ff{{end}}",
            "{{if eq \"Discharging\" .State.String}}#ff5722{{end}}",
            "{{if eq \"Full\" .State.String}}#4caf50{{end}}"
          ],
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }} ",
          "properties": {
            "discharging_icon": " ",
            "charging_icon": " ",
            "charged_icon": " "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "nix-shell",
          "style": "diamond",
          "foreground": "p:color13",
          "background": "transparent",
          "template": "(nix-{{ .Type }})"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "type": "docker",
          "style": "diamond",
          "foreground": "p:color13",
          "background": "#0B59E7",
          "template": "  {{ .Context }} "
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "type": "kubectl",
          "style": "diamond",
          "foreground": "p:color13",
          "background": "#ebcc34",
          "template": " ﴱ {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} ",
          "properties": {
            "context_aliases": {
              "arn:aws:eks:eu-west-1:1234567890:cluster/posh": "posh"
            }
          }
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "type": "deno",
          "style": "diamond",
          "foreground": "p:color13",
          "background": "#3C82F6",
          "template": "  {{ .Full }} "
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "type": "bun",
          "style": "diamond",
          "foreground": "p:color13",
          "background": "#3C82F6",
          "template": "  {{ .Full }} "
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "type": "cmake",
          "style": "diamond",
          "foreground": "p:color13",
          "background": "#1E9748",
          "template": "   cmake {{ .Full }} "
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "type": "angular",
          "style": "diamond",
          "foreground": "p:color13",
          "background": "#1976d2",
          "template": "  {{ .Full }} "
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "  {{ .Full }} ",
          "foreground": "p:color13",
          "type": "react",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "  {{ .Full }} ",
          "foreground": "p:color13",
          "background": "#1976d2",
          "type": "svelte",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "  {{ .Full }} ",
          "foreground": "p:color13",
          "background": "#1976d2",
          "type": "tauri",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " 󰄛 {{ .Full }} ",
          "foreground": "p:color13",
          "background": "#2E2A65",
          "type": "yarn",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "  {{ .Full }} ",
          "foreground": "p:color13",
          "background": "#06A4CE",
          "type": "flutter",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " 󰥧 {{ .Project }} ",
          "foreground": "p:color13",
          "background": "#FFA000",
          "type": "firebase",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " Maven {{ .Full }} ",
          "foreground": "p:color13",
          "background": "#2E2A65",
          "type": "mvn",
          "style": "diamond"
        },
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none",
            "display_changing_color": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "full_branch_path": true
          },
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " ({{ url .UpstreamIcon .UpstreamURL }} {{ url .HEAD .UpstreamURL }}){{ if gt .Ahead 0 }}<#50fa7b> +{{ .Ahead }}</>{{ end }}{{ if gt .Behind 0 }}<#ff5555> -{{ .Behind }}</>{{ end }}{{ if .Working.Changed }}<p:color15>  {{ .Working.String }}</>{{ end }}{{ if .Staging.Changed }}<p:color15>  {{ .Staging.String }}</>{{ end }}",
          "foreground": "p:color13",
          "background": "#ffb86c",
          "type": "git",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " {{ .Full }}",
          "foreground": "p:color13",
          "background": "#50fa7b",
          "type": "node",
          "style": "diamond"
        },
        {
          "properties": {
            "display_mode": "files",
            "extensions": [
              "package-lock.json"
            ]
          },
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " {{ .Full }}",
          "foreground": "p:color13",
          "background": "#ff5555",
          "type": "npm",
          "style": "diamond"
        },
        {
          "properties": {
            "display_mode": "files",
            "extensions": [
              "pnpm-lock.yaml"
            ]
          },
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "󰋁 {{ .Full }}",
          "foreground": "p:color13",
          "background": "#ffb86c",
          "type": "pnpm",
          "style": "diamond"
        },
        {
          "properties": {
            "fetch_virtual_env": true,
            "display_mode": "environment",
            "home_enabled": true
          },
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "foreground": "p:color13",
          "background": "#50fa7b",
          "type": "python",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " {{ .Full }}",
          "foreground": "p:color13",
          "background": "#ff79c6",
          "type": "java",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "  {{ .Full }} ",
          "foreground": "p:color13",
          "background": "#4063D8",
          "type": "php",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "  {{ .Full }} ",
          "foreground": "p:color13",
          "background": "#7FD5EA",
          "type": "go",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "  {{ .Full }} ",
          "foreground": "p:color13",
          "background": "#00ffff",
          "type": "dotnet",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "  {{ .Full }} ",
          "foreground": "p:color13",
          "background": "#99908a",
          "type": "rust",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "  {{ .Full }} ",
          "foreground": "p:color13",
          "background": "#4063D8",
          "type": "ruby",
          "style": "diamond"
        },
        {
          "type": "swift",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#ffffff",
          "background": "#f6553c",
          "template": "  {{ .Full }} "
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " <b>K</b> {{ .Full }} ",
          "foreground": "p:color13",
          "background": "#906cff",
          "type": "kotlin",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "  {{ .Full }} ",
          "foreground": "p:color13",
          "background": "#0000ff",
          "type": "lua",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "  {{ .Full }} ",
          "foreground": "p:color13",
          "background": "#422251",
          "type": "elixir",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "  {{ .Full }} ",
          "foreground": "p:color13",
          "background": "#100e23",
          "type": "haskell",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " {{ .Full }}",
          "foreground": "p:color13",
          "background": "#000000",
          "type": "crystal",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "type": "dart",
          "style": "diamond",
          "foreground": "p:color13",
          "background": "#06A4CE",
          "template": "  {{ .Full }} "
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "  {{ .Full }} ",
          "foreground": "p:color13",
          "background": "#4063D8",
          "type": "julia",
          "style": "diamond"
        },
        {
          "type": "perl",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#ffffff",
          "background": "#4063D8",
          "template": "  {{ .Full }}"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " {{ .Full }}",
          "foreground": "p:color15",
          "background": "#f7a41d",
          "type": "zig",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " 󱈚 {{ .Full }} ",
          "foreground": "p:color15",
          "background": "#422251",
          "type": "fortran",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " 🔥 {{ .Full }} ",
          "foreground": "p:color13",
          "background": "#906cff",
          "type": "mojo",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "  {{ .Full }} ",
          "foreground": "p:color13",
          "background": "#f3d400",
          "type": "nim",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "  {{ .Full }} ",
          "foreground": "p:color13",
          "background": "#d08770",
          "type": "ocaml",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "  {{ .Full }} ",
          "foreground": "p:color13",
          "background": "#4F87FF",
          "type": "v",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "  {{ .Full }} ",
          "foreground": "p:color13",
          "background": "#5E20A4",
          "type": "vala",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "p:color13",
          "background": "#ffad55",
          "type": "zig",
          "style": "diamond"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "type": "aws",
          "style": "diamond",
          "foreground": "p:color13",
          "background": "#FFA400",
          "template": "  {{.Profile}}{{if .Region}}@{{.Region}}{{end}}"
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "type": "az",
          "style": "diamond",
          "foreground": "p:color13",
          "background": "#9ec3f0",
          "template": "  {{ .EnvironmentName }}",
          "properties": {
            "source": "pwsh"
          }
        },
        {
          "leading_diamond": "",
          "trailing_diamond": "",
          "type": "azd",
          "style": "diamond",
          "foreground": "p:color13",
          "background": "#9ec3f0",
          "template": "  {{ .DefaultEnvironment }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "╰─ ❯❯",
          "foreground": "p:color15",
          "type": "text",
          "style": "diamond"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "patch_pwsh_bleed": true,
  "final_space": true,
  "palette": {
    "color0": "#0f1417",
    "color1": "",
    "color2": "",
    "color3": "",
    "color4": "",
    "color5": "",
    "color6": "",
    "color7": "",
    "color8": "",
    "color9": "",
    "color10": "#8dcff1",
    "color11": "",
    "color12": "",
    "color13": "#001e2b",
    "color14": "",
    "color15": "#dfe3e7"
  }
}
